{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "clean": "rimraf _site",
    "build": "ELEVENTY_ENV=production npm-run-all clean -s build:*",
    "build:rollup": "rollup -c",
    "build:11ty": "eleventy",
    "build:postcss": "postcss _site/**/*.css --dir _site/",
    "serve": "ELEVENTY_ENV=development npm-run-all clean -p serve:*",
    "serve:rollup": "rollup -c -w",
    "serve:11ty": "eleventy --serve",
    "debug": "ELEVENTY_ENV=debug DEBUG=* eleventy",
    "js:lint": "eslint './**/*.{js,njk}'",
    "js:fix": "eslint --fix './**/*.{js,njk}'",
    "css:lint": "stylelint './**/*.{css,njk}'",
    "css:fix": "stylelint --fix './**/*.{css,njk}'",
    "format": "prettier --write './**/*.{js,njk,css}'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/parser": "^7.12.3",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@visionappscz/stylelint-config-visionapps": "^1.1.0",
    "@visionappscz/stylelint-config-visionapps-order": "^1.1.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-minifier": "^4.0.0",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-cli": "^8.2.0",
    "postcss-load-config": "^3.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.27.0",
    "stylelint": "^13.7.2",
    "stylelint-find-rules": "^2.2.0",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0"
  },
  "dependencies": {
    "eleventy-plugin-pwa": "^1.0.8",
    "minilazyload": "^2.3.4"
  }
}
